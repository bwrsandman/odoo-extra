<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

    <record id="view_branch_form" model="ir.ui.view">
        <field name="model">runbot.branch</field>
        <field name="inherit_id" ref="runbot.view_branch_form"/>
        <field name="arch" type="xml">
          <group string="Params">
            <field name="is_addon_repo" invisible="True"/>
            <field name="odoo_core_branch" attrs="{'invisible': [('is_addon_repo', '=', False)]}" domain="['&amp;', ('is_addon_repo', '=', False)]"/>
          </group>
        </field>
    </record>

</data>
</openerp>
